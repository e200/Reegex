<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="MinimalBorderColor">#CCC</Color>
    <SolidColorBrush x:Key="MinimalBorderBrush" Color="{StaticResource MinimalBorderColor}"/>
    <Color x:Key="MinimalBorderHoverColor">#FFFF9494</Color>
    <SolidColorBrush x:Key="MinimalBorderHoverBrush" Color="{StaticResource MinimalBorderHoverColor}"/>

</ResourceDictionary>